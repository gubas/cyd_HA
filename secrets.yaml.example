# Your Wi-Fi SSID and password
wifi_ssid: "YOUR_WIFI_SSID"
wifi_password: "YOUR_WIFI_PASSWORD"

# API Encryption Key for cyd_ha device
# Generate one with: openssl rand -base64 32
cyd_ha_api_encryption_key: "YOUR_API_ENCRYPTION_KEY"

# OTA Update Password for cyd_ha device
cyd_ha_ota_password: "YOUR_OTA_PASSWORD"

# AP Fallback (when WiFi fails)
cyd_ha_ap_ssid: "CYD HA Fallback Hotspot"
cyd_ha_ap_password: "YOUR_AP_PASSWORD"
